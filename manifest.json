{
  "name": "Gmail Checker",
  "description": "Chrome extension that checks unread emails.",
  "version": "0.0.1",
  "manifest_version": 2,
  "permissions": [
    "*://*.google.com/",
    "alarms",
    "storage",
    "tabs",
    "webNavigation"
  ],
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "options_page": "options.html",
  "browser_action": {
    "default_title": "Gmail Checker",
    "default_icon": "gmail_not_ready.png"
  },
  "icons": {
    "128": "gmail_icon_128.png"
  }
}
